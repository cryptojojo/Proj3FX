<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="778.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
	<children>
		<MenuBar prefHeight="0.0" prefWidth="629.0" VBox.vgrow="NEVER">
			<menus>
				<Menu fx:id="about" mnemonicParsing="false" onAction="#handleAbout" text="About">
					<items>
						<MenuItem mnemonicParsing="false" text="About Elevator" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<Pane prefHeight="390.0" prefWidth="640.0">
			<children>
				<Label layoutX="14.0" layoutY="14.0" text="Number of Floors">
					<font>
						<Font size="16.0" />
					</font>
				</Label>
				<TextField fx:id="numOfFloors" layoutX="158.0" layoutY="14.0" prefHeight="25.0" prefWidth="34.0" />
				<Button fx:id="submitButton" layoutX="284.0" layoutY="309.0" mnemonicParsing="false" onAction="#handleSubmit" text="Submit">
					<font>
						<Font size="16.0" />
					</font>
				</Button>
            <Label layoutX="14.0" layoutY="52.0" text="Number of Riders added per floor">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="numOfRidersMin" layoutX="267.0" layoutY="52.0" prefHeight="25.0" prefWidth="34.0" />
            <TextField fx:id="numOfRidersMax" layoutX="339.0" layoutY="52.0" prefHeight="25.0" prefWidth="34.0" />
            <Label layoutX="310.0" layoutY="52.0" text="to">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label layoutX="13.0" layoutY="86.0" text="Percentage of riders which are VIP">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="vip" layoutX="267.0" layoutY="86.0" prefHeight="25.0" prefWidth="34.0" />
            <Label layoutX="311.0" layoutY="85.0" text="\%">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
			</children>
		</Pane>
      <TextArea fx:id="output" prefHeight="366.0" prefWidth="640.0" />
	</children>
</VBox>
